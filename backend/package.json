        "@typescript-eslint/parser": "^8.22.0",
        "cross-env": "^7.0.3",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "jest": "^29.7.0",
        "nodemon": "^3.1.9",
        "pino-caller": "^3.4.0",
        "prettier": "^3.4.2",
        "rimraf": "^6.0.1",
