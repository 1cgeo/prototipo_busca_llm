{
    "name": "geospatial-search-backend",
    "version": "1.0.0",
    "description": "Backend para o sistema de busca de dados cartogrÃ¡ficos",
    "main": "dist/index.js",
    "type": "module",
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "scripts": {
        "start": "node dist/index.js",
        "dev": "nodemon",
        "build": "tsc",
        "type-check": "tsc -p tsconfig.json --noEmit && tsc -p tsconfig.test.json --noEmit",
        "type-check:watch": "tsc -p tsconfig.test.json --noEmit --watch",
        "lint": "eslint \"src/**/*.ts\"",
        "lint-full": "npm run type-check && npm run lint",
        "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles",
        "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
        "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
        "evaluate": "cross-env NODE_OPTIONS=--experimental-vm-modules node --loader ts-node/esm evaluation/index.ts",
        "knip": "knip"
    },
    "dependencies": {
        "@instructor-ai/instructor": "^1.7.0",
        "chalk": "^5.4.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "helmet": "^8.0.0",
        "openai": "^4.82.0",
        "pg-promise": "^11.10.2",
        "pino": "^9.6.0",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/geojson": "^7946.0.16",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.1",
        "@typescript-eslint/eslint-plugin": "^8.22.0",
        "@typescript-eslint/parser": "^8.22.0",
        "cross-env": "^7.0.3",
        "eslint": "^9.19.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.3",
        "jest": "^29.7.0",
        "knip": "^5.44.0",
        "nodemon": "^3.1.9",
        "prettier": "^3.4.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3"
    }
}
